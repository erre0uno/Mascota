@page
@model MascotaFeliz.App.Front.Pages.ListaModel
@{
}

<h1>Listado de prueba </h1>
<table class="table">
    @foreach (var item in Model.Duenos)
    {   
        <tr>
            <td>@item.Nombres  </td>
            <td>@item.Apellidos  </td>
            <td>@item.Direccion  </td>
            <td>@item.Correo  </td>
            <td>
                <a class="btn btn-primary table-btn" asp-page="./Detalles" asp-route-duenoId="@item.PersonaID"  >
                Detalles de dueño </a>
            </td>
        </tr>
    }
</table>

